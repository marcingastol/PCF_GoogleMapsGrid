<?xml version="1.0" encoding="utf-8" ?>
<manifest>
  <control namespace="jPCF" constructor="GoogleMapsControl" version="0.0.6" display-name-key="GoogleMapsGM1" description-key="GoogleMapsGM1_desc" control-type="standard">
    <!-- dataset node represents a set of entity records on CDS; allow more than one datasets -->
    <data-set name="mapDataSet1" display-name-key="mapDataSet_Display_Key">
      <!-- 'property-set' node represents a unique, configurable property that each record in the dataset must provide. -->
      <!-- UNCOMMENT TO ADD PROPERTY-SET NODE
      <property-set name="samplePropertySet" display-name-key="Property_Display_Key" description-key="Property_Desc_Key" of-type="SingleLine.Text" usage="bound" required="true" />
      -->
    </data-set>
    
    <property name="primaryFieldName" display-name-key="MapGrid_primaryField_Display_Key" description-key="MapGrid_primaryField_Desc_Key" of-type="SingleLine.Text" usage="input" required="true"  default-value="name"/>
    <property name="Coordinates" display-name-key="Coordinates" description-key="Coordinates" of-type="Multiple" usage="input" required="true" default-value="coordinates"/>
    <property name="Latitude" display-name-key="Latitude" description-key="Latitude" of-type="SingleLine.Text" usage="input" required="true"  default-value="address1_latitude"/>
    <property name="Longitude" display-name-key="Longitude" description-key="Longitude" of-type="SingleLine.Text" usage="input" required="true" default-value="address1_longitude"/>
    <property name="GoogleMapsApiKey" display-name-key="GoogleMapsApiKey" description-key="GoogleMapsApiKey" of-type="SingleLine.Text" usage="input" required="true"  />
    <resources>
      <code path="index.ts" order="1"/>
    </resources>
    <!-- UNCOMMENT TO ENABLE THE SPECIFIED API
    <feature-usage>
      <uses-feature name="Device.captureAudio" required="true" />
      <uses-feature name="Device.captureImage" required="true" />
      <uses-feature name="Device.captureVideo" required="true" />
      <uses-feature name="Device.getBarcodeValue" required="true" />
      <uses-feature name="Device.getCurrentPosition" required="true" />
      <uses-feature name="Device.pickFile" required="true" />
      <uses-feature name="Utility" required="true" />
      <uses-feature name="WebAPI" required="true" />
    </feature-usage>
    -->
  </control>
</manifest>